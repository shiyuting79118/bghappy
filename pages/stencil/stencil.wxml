<!--pages/stencil/stencil.wxml-->
<view class="stencil">
  <view class='stencil-title'>
    <text class="{{tabnum==0?'stencil-title-tap':''}}" bindtap='tapTitleBar' data-index="0">免费模版</text>
    <text class="{{tabnum==1?'stencil-title-tap':''}}" bindtap='tapTitleBar' data-index="1">付费模版</text>
  </view>
  <view>
    <view class='stencil-body'>
      <view wx:for="{{array}}" wx:for-item="item" wx:key="key" class='stencil-item'>
        <view>
          <image style="width: 100px; height: 100px;" src="../../images/logo.jpeg"></image>
        </view>
        <view class="stencil-item-title">
          <text >卡通可爱</text>
        </view>

      </view>
    </view>
  </view>
</view>